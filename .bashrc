alias gd="git diff --minimal --ignore-all-space"
alias gf="git fetch"
alias gl="git log --oneline --graph"
alias gla="git log --oneline --graph --all"
alias gs="git status"
alias gsw="git switch"
alias ga="git add"
alias gb="git switch -c"

alias ll="ls -lA --time-style=long-iso -h --group-directories-first -X -G"

